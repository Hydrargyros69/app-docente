@model IEnumerable<AppDocentes.Models.Sede>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Comuna)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Direccion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NomSede)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NomCiudad)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Comuna)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Direccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NomSede)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NomCiudad)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdSede">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdSede">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdSede">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
