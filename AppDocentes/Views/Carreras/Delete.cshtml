@model AppDocentes.Models.Carrera

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h3>¿Esta seguro de eliminar esta Carrera?</h3>
<div>

    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.NomCarrera)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NomCarrera)
        </dd>
    </dl>
    <hr />

    <form asp-action="Delete">
        <input type="hidden" asp-for="IdCarrera" />
        <input type="submit" value="Eliminar" class="btn btn-danger" />
        <a asp-action="Index" class="btn btn-secondary">Volver Atras</a>
    </form>
</div>
